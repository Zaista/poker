{
  "compilerOptions": {
    "target": "ES6",
    "module": "esnext",
    "types": ["node", "express"],
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "exclude": ["e2e", "cypress.config.ts"]
}
